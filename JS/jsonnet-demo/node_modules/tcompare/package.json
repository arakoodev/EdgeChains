  "description": "A comprehensive comparison library, for use in test frameworks",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tapjs/tcompare.git"
  },
  "author": "Isaac Z. Schlueter <i@izs.me> (https://izs.me)",
  "license": "ISC",
  "files": [
    "index.js",
    "lib/*.js"
  ],
  "dependencies": {
    "diff": "^4.0.2"
  },
  "devDependencies": {
    "tap": "^15.0.8"
